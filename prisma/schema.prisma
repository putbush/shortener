generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          Int       @id @default(autoincrement())
  originalUrl String
  code        String    @unique()
  createdAt   DateTime  @default(now())
  expiresAt   DateTime?
  visits      Int       @default(0)
}
